    "import cv2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "7e82d2a9",
   "metadata": {},
   "outputs": [],
   "source": [
    "abc = cv2.VideoCapture(0)\n",
    "\n",
    "while(True):\n",
    "    \n",
    "    ret , frame = abc.read()\n",
    "    \n",
    "    cv2.imshow(\"abc\",frame)\n",
    "    \n",
    "    if cv2.waitKey(1) & 0xFF == ord('q'):\n",
    "        \n",
    "        break\n",
    "        abc.release()\n",
    "        \n",
    "       \n",
    "cv2.destroyAllWindows()"
   ]
